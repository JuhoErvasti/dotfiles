#/usr/bin/env bash

function _start_dev_completions {
  first_reply="c cpp python rust"
  if [ "${#COMP_WORDS[@]}" == "2" ]; then
      COMPREPLY=($(compgen -W "$first_reply" "${COMP_WORDS[1]}"))
      return
  else
      COMPREPLY=($(compgen -o default "${COMP_WORDS[2]}"))
  fi
}

complete -F _start_dev_completions ___start_dev
