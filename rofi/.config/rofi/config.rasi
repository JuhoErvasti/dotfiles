configuration{
  modi: "combi,run,window,drun";
  combi-modi: "window,drun,run";
  icon-theme: "Oranchelo";
  show-icons: true;
  terminal: "alacritty";
  drun-display-format: "{name} {icon}";
  location: 0;
  disable-history: false;
  hide-scrollbar: true;
  display-combi: "All";
  display-run: "Programs";
  display-drun: "Desktop Files";
  display-window: "Window";
  sidebar-mode: true;
  drun {
     scan-desktop: true;
     parse-user:   true;
     parse-system: true;
  }
}
